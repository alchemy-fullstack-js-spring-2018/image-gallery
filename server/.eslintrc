{
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "env": {
      "node": true,
      "mocha": true,
      "es6": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-console": "warn",
      "indent": ["error", 4],
      "quotes": ["error", "single"],
      "semi": ["error", "always"]
    }
}