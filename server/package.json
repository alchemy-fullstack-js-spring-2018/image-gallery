{
  "name": "server",
  "version": "1.0.0",
  "engines": {
    "node": "8.x"
  },
  "description": "Image Gallery Back End",
  "main": "server.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "lint": "eslint lib test",
    "pretest": "npm run lint",
    "test:mocha": "mocha  --recursive",
    "test": "npm run test:mocha",
    "test:unit": "mocha test/unit",
    "test:e2e": "mocha test/e2e",
    "test:watch": "nodemon --exec 'npm run test:mocha'",
    "test:e2e:watch": "nodemon --exec 'npm run test:e2e'",
    "test:unit:watch": "nodemon --exec 'npm run test:unit'",
    "start": "node server.js",
    "start:watch": "nodemon server.js"
  },
  "keywords": [],
  "authors": [
    "Keli Hansen",
    "Steele Walston"
  ],
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "eslint": "^4.19.1",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.3"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.2.1",
    "mongoose": "^5.0.16",
    "morgan": "^1.9.0"
  }
}
